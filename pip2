#  Write a Python function that takes a list of words and returns the longest word and the length of the longest one
def longest_word(word):
   emptyArray=[]
   longest_word=0
   for i in word:
      if len(i)>longest_word:
         
         emptyArray=i
         longest_word=len(i)
         
Clothes=["dress","sweater","palazo"] 
print(longest_word(Clothes))

